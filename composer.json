{
    "name": "igoooor/stackdriver-bundle",
    "description": "Stackdriver Monitoring",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "google/cloud-error-reporting": "0.*",
        "google/cloud-logging": "^1.24.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/monolog-bundle": "^3.8",
        "symfony/security-core": "^6.1",
        "symfony/http-kernel": "^6.1",
        "google/cloud-core": "^1.46",
        "monolog/monolog": "^3.1",
        "psr/log": "^3.0"
